extends layout

block content
  h1= title
  - var items  = results;
  div(class="container-fluid")
  div(class="row m-1")
   each v,index  in items 
    - let destination = `dispatch(\"/restaurant/update?id=${v._id}\")`
    div(class="col-sm-2" class="m-1"  ) #{v.name}, #{v.place}, #{v.bill}
        button.btn( class="btn-primary ml-1", onclick= destination) Update
      
  script.
   function dispatch(path){
        window.location.href=path
     }
       































































  